    {
      "type": "text",
      "version": 87,
      "versionNonce": 1348031885,
      "isDeleted": false,
      "id": "u_SCLKCYpkTk66kZsN6DP",
      "fillStyle": "solid",
      "strokeWidth": 1,
      "strokeStyle": "solid",
      "roughness": 1,
      "opacity": 100,
      "angle": 0,
      "x": 286,
      "y": 9307.129760742188,
      "strokeColor": "#1971c2",
      "backgroundColor": "transparent",
      "width": 1281.0072021484375,
      "height": 360,
      "seed": 142727448,
      "groupIds": [],
